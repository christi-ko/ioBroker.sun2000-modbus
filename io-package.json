        "keywords": [
            "inverter",
            "huawei",
            "modbus",
            "luna2000",
            "sun2000"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "main": "build/main.js",
        "icon": "sun2000-modbus.png",
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/daolis/ioBroker.sun2000-modbus/main/admin/sun2000-modbus.png",
        "readme": "https://github.com/daolis/ioBroker.sun2000-modbus/blob/main/README.md",
        "loglevel": "info",
        "mode": "daemon",
        "type": "hardware",
        "compact": true,
        "connectionType": "local",
        "dataSource": "poll",
        "adminUI": {
            "config": "json"
        },
        "dependencies": [
            {
                "js-controller": ">=3.3.22"
            }
        ],
        "globalDependencies": [
            {
                "admin": ">=5.1.13"
            }
        ],
        "adminColumns": [
            {
                "name": {
                    "en": "Inverter Adress",
                    "de": "Wechselrichter Adresse"
                },
                "path": "native.address",
                "width": 500,
                "align": "left"
            },
            {
                "name": {
                    "en": "Port",
                    "de": "Port"
                },
                "path": "native.port",
                "width": 100,
                "align": "left"
            },
            {
                "name": {
                    "en": "Modbus unitID",
                    "de": "Modbus unitID"
                },
                "path": "native.modbusUnitId",
                "width": 100,
                "align": "left"
            },
            {
                "name": {
                    "en": "Update interval",
                    "de": "Update Intervall"
                },
                "path": "native.updateInterval",
                "width": 100,
                "align": "left"
            }
        ]
    },
    "native": {
        "address": "10.1.3.20",
        "port": 502,
        "modbusUnitId": 1,
        "updateInterval": 5
    },
    "objects": [],
    "instanceObjects": [
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information",
                "role": "info"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "name": "Inverter connected",
                "type": "boolean",
                "role": "indicator.connected",
                "read": true,
                "write": false,
                "desc": "Is the inverter connected?"
            }
        },
        {
            "_id": "info.ip",
            "type": "state",
            "common": {
                "name": "Inverter IP",
                "type": "string",
                "role": "indicator.ip",
                "read": true,
                "write": false,
                "desc": "Inverter IP address"
            }
        },
        {
            "_id": "info.port",
            "type": "state",
            "common": {
                "name": "Modbus port",
                "type": "number",
                "role": "indicator.port",
                "read": true,
                "write": false,
                "desc": "Inverter modbus port"
            }
        },
        {
            "_id": "info.unitID",
            "type": "state",
            "common": {
                "name": "Modbus unit ID",
                "type": "number",
                "role": "indicator.id",
                "read": true,
                "write": false,
                "desc": "Inverter modbus unit id"
            }
        },
        {
            "_id": "info.modbusUpdateInterval",
            "type": "state",
            "common": {
                "name": "Modbus update interval",
                "type": "number",
                "role": "value",
                "read": true,
                "write": true,
                "desc": "Inverter modbus update interval",
                "unit": "sec"
            }
        },
        {
            "_id": "storage",
            "type": "channel",
            "common": {
                "name": "Storage data",
                "role": "info",
                "read": true,
                "write": false,
                "desc": "Storage 'realtime' data"
            },
            "native": {}
        },
        {
            "_id": "grid",
            "type": "channel",
            "common": {
                "name": "Grid"
            },
            "native": {}
        }
    ]
}
